10 REM *****ROOM SETPOINT ***************************
11 IF OCCUPIED THEN START AHU2OM
12 IF  NOT OCCUPIED THEN STOP AHU2OM
20 IF AHU2SCH OR AHU2OM THEN  AHU2SP = AHU2DSP ELSE  AHU2SP = NITESP
30  AHU2HSP = AHU2SP - 2
40  AHU2CSP = AHU2SP + 2
50 REM ********** NIGHT WARMUP ***************
60 IF  NOT OCCUPIED AND AHU2SAT < NITESP THEN START AHU1WUM
70 IF OCCUPIED AND AHU2SAT < AHU2HSP - 3 THEN START AHU1WUM
80 IF AHU2SAT > AHU2HSP THEN STOP AHU1WUM
90 IF TIME-ON( OCCUPIED ) > 00:30:00 THEN STOP AHU1WUM
100 REM ******** COOLING MODE *****************
110 IF TIME-OFF( HEATMOD2 ) < 01:00:01 THEN GOTO 140
120 IF OAT < 10 THEN GOTO 140
130 IF AHU2SAT > AHU2SP + 1 THEN START COOLMOD2
140 IF AHU2SAT < AHU2SP THEN STOP COOLMOD2
150 IF  NOT OCCUPIED AND  NOT AHU2OM THEN STOP COOLMOD2
160 REM ******** HEATING MODE ********
170 IF AHU2SAT < AHU2SP THEN START HEATMOD2
180 IF AHU2SAT > AHU2SP + 0.50 THEN STOP HEATMOD2
190 IF HEATMOD2 THEN STOP COOLMOD2
200 REM ****** FAN ********
210 IF RM401OM OR AHU2SCH OR AHU2OM THEN START AHU2-FAN ELSE STOP AHU2-FAN
220 IF AHU1WUM THEN START AHU2-FAN
221 IF AHU2SMOK THEN STOP AHU2-FAN
230 REM ************** VALVE *************************
240  AHU2-VLV = 1-CON6
250 IF COOLMOD2 THEN  AHU2-VLV = 1-CON7
260 IF AHU1WUM THEN  AHU2-VLV = 100
270 IF AHU2SMOK OR  NOT AHU2S THEN  AHU2-VLV = 0
290 REM ***
