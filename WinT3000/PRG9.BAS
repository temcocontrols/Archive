10 REM ********* ELEVATOR PROGRAM ********
20 IF GENALARM THEN  ELCALL = 1
30 IF  NOT OCCUPIED THEN  ELCALL = 1
40 IF ELCALL > ELFLOOR THEN  ELDIR = 1
50 IF ELCALL = ELFLOOR THEN  ELDIR = 0
60 IF ELCALL < ELFLOOR THEN  ELDIR =  -1
65 REM ******* START THE OUTPUTS *********
70 IF ELDIR =  -1 THEN START ELEV-DN
80 IF ELDIR = 0 THEN START ELEV-DN
90 IF ELDIR = 1 THEN STOP ELEV-DN
100 IF ELDIR =  -1 THEN START ELEV-UP
110 IF ELDIR = 0 THEN STOP ELEV-UP
120 IF ELDIR = 1 THEN STOP ELEV-UP
125 REM ****** WATCH THE INPUTS ***********
130 IF- FLOOR1 THEN  ELFLOOR = 1
130 IF- FLOOR3 THEN  ELFLOOR = 3
130 IF- FLOOR4 THEN  ELFLOOR = 4
