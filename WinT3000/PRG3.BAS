10 REM ************* FAN STATUS **************
20  AHU1S = TIME-ON( AHU1-FAN )
30 REM ********* SIMULATED MIXED AIR TEMP *******
31  AHU1-MAT = (AHU1RAT * (100 - AHU1MAD) + OAT * AHU1MAD) / 100
30 REM ********* SIMULATED SUPPLY AIR TEMP *******
100 IF HEATMODE THEN  RISE1 = AHU1VLV / 4
100 IF COOLMODE THEN  RISE1 =  -AHU1VLV / 1
120  RISE1 = MAX(  -10 , MIN( 10 , RISE1 ) )
121  AHU1-SAT = AHU1-MAT + RISE1
122  AHU1-SAT = MAX( 0 , MIN( 40 , AHU1-SAT ) )
220 REM ************ FREEZE ********************
230 REM IF OAT < 0 AND AHU1-MAD > 30 AND AHU1-FAN THEN START AHU1-FZ
240 REM IF OAT > 4 THEN STOP AHU1-FZ
250 REM **************ROOM TEMPS******************
260  AIRFLOW1 = 50 * AHU1S
280 REM ******** GAIN1: SOLAR **************
290 IF DAYTIMER = 0 THEN  GAIN1 = 0
300 IF DAYTIMER > 2000 THEN  GAIN1 = GAIN1 - OAT * 0.10
310 IF DAYTIMER > 600 AND DAYTIMER < 20000 THEN  GAIN1 = GAIN1 + OAT * 0.10
320  GAIN1 = MIN( GAIN1 , 10 * OAT )
280 REM ******** GAIN2: AIR IN **************
370  GAIN2 = 10 * AIRFLOW1 * AHU1-SAT
390 REM ************ GAIN3: INTERNAL*********
400 IF OCCUPIED OR AHU1OM THEN  GAIN3 = 150
410 IF  NOT OCCUPIED AND  NOT AHU1OM THEN  GAIN3 = 0
430 REM ******* LOSS1:THERMAL ***************
440  LOSS1 = 100 * (AHU1RAT - OAT)
460 REM ********** LOSS2:RETURN AIR *********
470  LOSS2 = AHU1RAT * AIRFLOW1 * 0.87
480 REM ****** TOTALS ***********************
490  GAIN = GAIN1 + GAIN2 + GAIN3
500  LOSS = LOSS1 + LOSS2
510  ENERGY1 = GAIN - LOSS
511 REM ************ TEMP CHANGE IN ROOM ****************
520  1-VAR127 = ENERGY1 / 1000
521 REM RTSC1 = .1
521  RTSC1 = (RTSC1 * (TIMEBASE - 1) + 1-VAR127) / TIMEBASE
530  RTSC1 = MAX(  -0.10 , MIN( 0.10 , RTSC1 ) )
551  AHU1RAT = AHU1RAT + RTSC1 / 1
560  AHU1RAT = MAX( 0 , MIN( 40 , AHU1RAT ) )
