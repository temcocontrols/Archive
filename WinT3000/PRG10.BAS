10 REM ******* COOLING TOWER HEAT MASS BALANCE ********
30 REM ******** ENERGY TAKEN OUT BY FANS **************
40  DELTATM = (1 + FANSREQD) * NUMPUMP2 * (CWST - OAT)
41 REM IF  NOT CH1 THEN  DELTATM = 0
90 REM IF NUMPUMP2 = 0 THEN  DELTATM = 0
50 REM DELTATM = MAX( 0 , MIN( DELTATM , 1 ) )
60 REM ********** ENERGY ADDED BY CHILLER***************
80  DELTATP = (1 - OAT / 20 + 1) / 6
90 REM IF NUMPUMP2 = 0 THEN  DELTATP = 0
100  DELTATP = MAX(  -15 , MIN( 15 , DELTATP ) )
110 REM ********** TEMPERATURES *********************
120  CWRT = (CWRT * 99 + CWST) / 100 - DELTATM
130  CWST = (CWST * 99 + CWRT) / 100 + DELTATP
170 REM **** LIMITS ******
180  CWST = MAX( 0 , MIN( 99 , CWST ) )
190  CWRT = MAX( 0 , MIN( 99 , CWRT ) )
201 IF HEATMODE THEN  CWST = MAX( CWST , OAT )
201 IF HEATMODE THEN  CWRT = MAX( CWRT , OAT )
